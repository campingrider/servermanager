<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <title>PHPDoc: RIDERs Server Manager</title>
  <parser>
    <default-package-name>de\campingrider\servermanager</default-package-name>
    <encoding>utf-8</encoding>
    <target>./doc</target>
  </parser>
  <transformer>
    <target>./doc</target>
  </transformer>
  <transformations>
    <template name="clean" />
  </transformations>
  <files>
    <directory>./classes</directory>
  </files>
</phpdoc>
